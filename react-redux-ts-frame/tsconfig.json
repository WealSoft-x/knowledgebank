{
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules"
    ],
    "compilerOptions": {
        "target": "ES2015",
        "module": "commonjs",
        "lib": ["esnext", "dom"],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "react",
        "noImplicitAny": false,
        "moduleResolution": "node",
    },
}